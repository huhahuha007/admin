      <param name="x" type="double"/>
    </method>
    <method name="sin" returnType="double">
      <param name="x" type="double"/>
    </method>
    <method name="sqrt" returnType="double">
      <param name="x" type="double"/>
    </method>
    <method name="tan" returnType="double">
      <param name="x" type="double"/>
    </method>
